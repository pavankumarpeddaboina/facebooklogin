package selenium;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class java {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		
		WebDriver driver = new ChromeDriver();
		
		driver.get("https://www.google.com");
		Thread.sleep(2000);
		
		//for printing to a console ...System.out.println
		//how to get title of any webpage
		driver.getTitle();
		System.out.println(driver.getTitle());
		
		//how to get url
		
	    System.out.println(driver.getCurrentUrl());
	    
	    //to close the browser two types
	    
	   driver.close();      //it will close the current session of the browser
	    
	   // driver.quit();      //it will all session of the browser
	    
	    
		

	}

}
